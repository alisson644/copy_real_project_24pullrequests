(function($users) {
  var $new = $("<%= escape_javascript(@users) %>").find("a[rel=tooltip]").tooltip().end();

  $users.find('.js-user-list')
        .append($new)

  $users.find(".more")
        .replaceWith(
          "<%= escaoe_javascript(link_to_next_page @users,
                                                 t('more'),
                                                 params: params.permit(:year),
                                                 remote: true,
                                                 class: 'btn btn-success more') %>"
        );
})($(".js-user.pagination"));